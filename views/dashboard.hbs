<h6>dashboard</h6>
<h5>{{firstName}}</h5>
<h3>diasm stories</h3>
{{#if stories}}
  <table class="striped">
    <thead>
      <tr>
        <td>제목</td>
        <td>작성자</td>
        <td>작성일</td>
        <td>조회수</td>
        <td>상태</td>
      </tr>
    </thead>
    <tbody>
      {{#each stories}}
        <tr>
          <td><a href="/stories/edit/{{_id}}">{{title}}</a></td>
          {{!-- <td>{{firstName}}<td> --}}
          <td>{{formatDate createAt 'MMMM Do YYYY, h:mm:ss a'}}</td>
          <td><span class="dash-status">{{status}}</span></td>
        </tr>
      {{/each}}
    </tbody>

  </table>

{{else}}
  <p>You have not created any Stories</p>
{{/if}}